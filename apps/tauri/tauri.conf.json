{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "anki-next",
  "version": "0.1.0",
  "identifier": "fun.ikkz.anki-next",
  "build": {
    "frontendDist": "../",
    "devUrl": "http://localhost:4000",
    "beforeDevCommand": "echo",
    "beforeBuildCommand": "echo"
  },
  "app": {
    "windows": [
      {
        "title": "AnkiNext",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
