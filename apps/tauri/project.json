{
  "name": "anki-next-tauri",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm tauri build -c apps/tauri/tauri.conf.json",
        "cwd": "{workspaceRoot}"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm tauri build -c apps/tauri/tauri.conf.json",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "implicitDependencies": []
}
