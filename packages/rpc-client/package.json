{
  "name": "@uni-anki/rpc-client",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {},
  "nx": {
    "targets": {
      "prebuild": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./dist/target/client_generator/debug/client_generator"
        },
        "configurations": {
          "production": {
            "command": "./dist/target/client_generator/release/client_generator"
          }
        },
        "dependsOn": [
          "^build"
        ]
      }
    },
    "implicitDependencies": [
      "client_generator"
    ]
  }
}
