{
  "name": "@uni-anki/rpc-client",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "protobuf-ts": {
        "cache": true,
        "executor": "nx:run-commands",
        "inputs": [
          "upstream/anki"
        ],
        "outputs": [
          "{projectRoot}/src/generated/protobuf"
        ],
        "options": {
          "command": "pnpm protoc --ts_out ./src/generated/protobuf --proto_path ../../anki/proto --ts_opt ts_nocheck --ts_opt eslint_disable ../../anki/proto/anki/*.proto",
          "cwd": "{projectRoot}"
        }
      },
      "client-generator": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./dist/target/client_generator/debug/client_generator",
          "cwd": "{workspaceRoot}"
        },
        "configurations": {
          "production": {
            "command": "./dist/target/client_generator/release/client_generator"
          }
        },
        "dependsOn": [
          "^build"
        ]
      },
      "build": {
        "dependsOn": [
          {
            "projects": "client_generator",
            "target": "run"
          },
          "protobuf-ts",
          "client-generator",
          "^build"
        ]
      }
    },
    "implicitDependencies": [
      "client_generator"
    ]
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.6"
  },
  "dependencies": {
    "@protobuf-ts/runtime-rpc": "^2.9.6"
  }
}
